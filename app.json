{
  "expo": {
    "name": "AppName",
    "description": "This is a bill-splitter app built using React Native and Firebase",
    "slug": "bill-splitter-app",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/images/app-icon.png",
    "splash": {
      "image": "./src/images/launch.png",
      "resizeMode": "contain",
      "backgroundColor": "#473be7"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.almm.capstone"
    },
    "android": {
      "package": "com.almm.capstone"
    }
  }
}
